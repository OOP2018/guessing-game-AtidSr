<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="124.0" prefWidth="454.0" spacing="15.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.GameController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <children>
      <Pane prefHeight="104.0" prefWidth="404.0">
         <children>
            <Button fx:id="enterButton" layoutX="295.0" layoutY="65.0" mnemonicParsing="false" onAction="#enterClickHandler" text="Enter" />
            <TextField fx:id="guessTextFleld" layoutX="140.0" layoutY="65.0" onAction="#enterClickHandler" />
            <Label layoutX="26.0" layoutY="6.0" text="I'm thinking number between 1-100 Can you Guess?" />
            <Label layoutX="43.0" layoutY="69.0" text="Your Guess" />
            <Label fx:id="hint" layoutX="150.0" layoutY="35.0" />
            <Label fx:id="count" layoutX="14.0" layoutY="35.0" text="count : 0" />
            <Button layoutX="345.0" layoutY="65.0" mnemonicParsing="false" onAction="#giveUpHandler" prefHeight="25.0" prefWidth="59.0" text="Give UP" />
         </children>
      </Pane>
   </children>
</HBox>
